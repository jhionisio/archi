  <folder name="Motivation" id="id-2f7bdd65cbbd4f9a9e3ac655a9b1174c" type="motivation">
    <element xsi:type="archimate:Stakeholder" name="Presidente da software house (DIlan)" id="id-b79d2aafbd0e4bfbab326eae8313cd0c"/>
    <element xsi:type="archimate:Driver" name="Driver" id="id-715d03bff7b24706bf32ba3360bd598b"/>
    <element xsi:type="archimate:Driver" name="Acabar com a informalidade nos atendimentos do suporte que estão gerando perdas de clientes" id="id-701c035f2c5f45e4ab720deccdbf2619"/>
    <element xsi:type="archimate:Value" name="Aumentar a retenção de clientes atuais, evitando cancelamentos de contratos. Hoje a empresa perde aproximadamente R$200000 em média por ano com cancelamentos em função da insatisfação com o suporte técnico da empresa" id="id-c7a081b64a444d0596856f01316c1874"/>
    <element xsi:type="archimate:Value" name="Clientes satisfeitos vão ampliar negócios e indicar novos clientes. Hoje, captamos entorno de R$120000 com novos negócios com clientes satisfeitos ou clientes que vieram por indicação." id="id-91468c3ac68c460c95a18023732bae0d"/>
    <element xsi:type="archimate:Assessment" name="Permitir acompanhar todo o processo de movimentação de atendimentos de clientes na área de suporte esteja documentado em um sistema informotaziado digital." id="id-bebec86439fe4df3b7d9d3b3b59a1de8"/>
    <element xsi:type="archimate:Assessment" name="Permitir consultar os feedbacks dos clientes sobre o atendimento" id="id-fbc533ca1ede45919b8e9ec60d31d08b"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-41393a85241d4c4b9c6bd327abe8da6b"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-72221089c6f74b479abb6a207248ce37"/>
    <element xsi:type="archimate:Goal" name="Ter um mêcanismo informatizado para registrar os pedidos do suporte do cliente" id="id-fa7d549f813743bfad338619bc7187b7"/>
    <element xsi:type="archimate:Goal" name="Ter um mêcanismo informatizado para registrar as atividades de solução do chamado" id="id-60f05ff9e5864de8a42108fb4ac738ad"/>
    <element xsi:type="archimate:Goal" name="Ter um mecanismo de comunicação com o cliente para obter uma avaliação do atendimento" id="id-1d200e7b6d52408f815ff14c7fc87b49"/>
    <element xsi:type="archimate:Outcome" name="100% dos atendimentos registrados em sistema de informação, sendo que esta solução deve estar disponível a partir de 31/12/2023" id="id-00abf111c19a49159756a7beb1641538"/>
    <element xsi:type="archimate:Outcome" name="As avaliações devem ocorrer após o encerramento do atendimento de cada chamada sendo que esta função deve estar disponível três meses após a implantação do sistema de controle de chamados" id="id-d652d7a59251411fab80f8022b5087c0"/>
    <element xsi:type="archimate:Principle" name="Os chamados devem ser resolvidos o mais rapidamente possível portanto devem ser direcionados para grupos de especialistas, conforme o tipo de ocorrência" id="id-9d7d9c42355240f48a51cca5829fd5ea"/>
    <element xsi:type="archimate:Principle" name="O processo de solução deve trabalhar com regras de prazo de atendimento (SLA)" id="id-f8700e45aea144799ad8b74eb2c9523b"/>
    <element xsi:type="archimate:Principle" name="As pesquisas de satisfação devem ser disparadas sempre ao final de um atendimento (após solução do chamado)" id="id-90cd12aa20e04517a721ca59d0069295"/>
    <element xsi:type="archimate:Principle" name="A resposta pelo cliente é opcional" id="id-a11f5453b19f4bccbcf07bed19d0c93b"/>
  </folder>
